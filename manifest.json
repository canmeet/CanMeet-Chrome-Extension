{
    "manifest_version": 3,
    "name": "CanMeet",
    "version": "0.1",
    "description": "A meeting helper",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "cookies"
    ],
    "host_permissions": [
        "http://127.0.0.1/",
        "*://canmeet.github.io/*",
        "*://canmeet.herokuapp.com/*",
        "https://canmeet-img-proxy.slink.ren/*"
      ],
    "action": {
        "default_popup": "popup.html"
    }
}